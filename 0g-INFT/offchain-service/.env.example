# 0G INFT Environment Configuration
PRIVATE_KEY=your_private_key_here

# 0G Network RPC URLs  
NEWTON_RPC_URL=https://evmrpc-testnet.0g.ai
GALILEO_RPC_URL=https://evmrpc-testnet.0g.ai

# Network Chain IDs
NEWTON_CHAIN_ID=16600
GALILEO_CHAIN_ID=16602

# 0G Storage Configuration
ZG_STORAGE_RPC=https://evmrpc-testnet.0g.ai
ZG_STORAGE_INDEXER=https://indexer-storage-testnet-turbo.0g.ai
ZG_STORAGE_PRIVATE_KEY=your_private_key_here

# Fresh Storage Root Hash (from latest upload - update with your hash)
ENCRYPTED_URI=0g://storage/0x[your_storage_root_hash_here]
STORAGE_ROOT_HASH=0x[your_storage_root_hash_here]

# Contract Addresses (Update with your deployed contracts)
INFT_CONTRACT_ADDRESS=0x[your_inft_contract_address_here]
ORACLE_CONTRACT_ADDRESS=0x[your_oracle_contract_address_here]

# LLM Configuration (Phase 2 - Phala Network RedPill Integration)
LLM_PROVIDER=phala-redpill
LLM_HOST=https://api.red-pill.ai
LLM_MODEL=mistralai/mixtral-8x7b-instruct
LLM_TEMPERATURE=0.2
LLM_MAX_TOKENS=256
LLM_REQUEST_TIMEOUT_MS=30000
LLM_MAX_CONTEXT_QUOTES=25
LLM_DEV_FALLBACK=true
REDPILL_API_KEY=your_redpill_api_key_here
